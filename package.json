{
  "name": "csspool",
  "version": "1.0.0",
  "description": "A simple inline CSS gui",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && npm run dir && npm run js && npm run copy",
    "build:watch": "chokidar \"src/js/**/*.js\" -c \"npm run build\"",
    "dev": "npm run serve | npm run js:watch | npm run copy:watch",
    "clean": "rimraf dist",
    "lint": "eslint src/js/index.js",
    "js": "webpack --config webpack.config.js",
    "js:watch": "chokidar \"src/**/*.js\" -c \"npm run js\"",
    "copy": "copy src\\index.html dist\\index.html | copy src\\page.html dist\\page.html",
    "copy:watch": "chokidar \"src/**/*.html\" -c \"npm run copy\"",
    "dir": "mkdirp dist/js",
    "serve": "node ./task/serve dist 8385",
    "version": "next-version package.json src/config/index.js",
    "version:minor": "npm run version -- --minor",
    "version:major": "npm run version -- --major",
    "version:release": "release.bat",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-babili": "0.0.12",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "body-parser": "^1.17.1",
    "chokidar-cli": "^1.2.0",
    "eslint": "^3.19.0",
    "express": "^4.15.2",
    "mkdirp": "^0.5.1",
    "next-version": "^0.11.7",
    "open": "0.0.5",
    "rimraf": "^2.6.1",
    "serve-static": "^1.12.1",
    "webpack": "^2.4.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sjeiti/csspool.git"
  },
  "keywords": [
    "css",
    "gui"
  ],
  "author": "Ron Valstar",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/Sjeiti/csspool/issues"
  },
  "homepage": "https://github.com/Sjeiti/csspool#readme"
}
